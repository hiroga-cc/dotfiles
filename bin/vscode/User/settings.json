{
  "breadcrumbs.enabled": true, // パンくずリスト
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "all",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.git": false
  },
  "git.autofetch": true,
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "bashdb",
        "request": "launch",
        "name": "Bash-Debug (select script from list of sh files)",
        "cwd": "${fileDirname}",
        "program": "${file}",
        "args": []
      }
    ]
  },
  "window.zoomLevel": 0,
  "workbench.sideBar.location": "right",
  // languages
  "java.configuration.checkProjectSettingsExclusions": false,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.acceptSuggestionOnEnter": "off"
  },
  "[python]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[yaml]": {
    "editor.autoIndent": "none",
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "redhat.vscode-yaml",
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  // Python PEP8:
  // E501: 80 characters are not too long in my environment.
  "python.formatting.autopep8Args": [
    "--ignore=E501"
  ],
  "python.linting.pycodestyleArgs": [
    "--ignore=E501"
  ],
  "python.formatting.provider": "autopep8", // black is pre-release
  // Python: Linting: Using pylint and flake8 both. Ther overlaped some but some does not.
  // http://books.agiliq.com/projects/essential-python-tools/en/latest/linters.html
  // https://www.reddit.com/r/Python/comments/82hgzm/any_advantages_of_flake8_over_pylint/
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  // AWS CloudFormation
  "yaml.customTags": [
    "!Ref",
    "!Sub scalar",
    "!Sub sequence",
    "!Join sequence",
    "!FindInMap sequence",
    "!GetAtt scalar",
    "!GetAtt sequence",
    "!Base64 mapping",
    "!GetAZs",
    "!Select scalar",
    "!Select sequence",
    "!Split sequence",
    "!ImportValue",
    "!Condition",
    "!Equals sequence",
    "!And",
    "!If",
    "!Not",
    "!Or"
  ],
  "yaml.format.enable": true,
  "yaml.format.proseWrap": "never",
  "vsicons.projectDetection.disableDetect": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "cSpell.enableFiletypes": [
